{
    "manifest_version": 2,
    "name": "google maps redirect",
    "description":"Adds Maps to google search results and redirects to search for phrase in Google Maps site.",
    "version": "1.0",
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*"],
          "js": ["content.js"]
        }
      ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
        }
      }
}